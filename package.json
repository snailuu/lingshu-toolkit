{
  "name": "@cmtlyt/lingshu-toolkit",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/shared.js",
  "module": "./dist/shared.js",
  "types": "./dist/shared/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react.js"
    },
    "./react/*": {
      "import": "./dist/react/*"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared.js"
    },
    "./shared/*": {
      "import": "./dist/shared/*"
    }
  },
  "scripts": {
    "build": "rslib build",
    "build:docs": "rspress build",
    "check": "biome check --write",
    "dev:docs": "rspress dev",
    "format": "biome format --write",
    "test": "vitest --config=vitest.browser.config.ts --coverage.enabled --ui",
    "test:ci": "vitest run --config=vitest.browser.config.ts",
    "test:lib:ci": "vitest run",
    "prepare": "husky",
    "test:lib": "vitest --coverage.enabled --ui",
    "script:gen-file": "esno scripts/gen-file.ts"
  },
  "author": {
    "name": "cmtlyt",
    "email": "cmtlyt@163.com"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cmtlyt/unplugin-shadcn-registry-generate": "^0.1.3",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rslib/core": "^0.19.3",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "esno": "^4.8.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rspress": "^1.47.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "4.0.18",
    "vitest-browser-react": "2.0.2",
    "vitest-browser-vue": "2.0.2",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "vue": "^3.5.27"
  },
  "sideEffects": false,
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=18.0.0"
    },
    "packageManager": {
      "name": "pnpm"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cmtlyt",
    "lingshu-toolkit",
    "hook",
    "hooks",
    "shadcn",
    "shadcn-hook",
    "vue",
    "react",
    "vue-hook",
    "react-hook"
  ]
}